{
  "name": "myacct-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:self-service": "ng build --configuration production --base-href /self-service/ --deploy-url /self-service/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "storybook": "ng run myacct-ui:storybook",
    "build-storybook": "ng run myacct-ui:build-storybook",
    "pa11y": "start-server-and-test \"ng serve --configuration development --host 127.0.0.1 --port 4500\" http://127.0.0.1:4500 \"pa11y-ci --config pa11yci.config.json\"",
    "test:e2e": "start-server-and-test \"ng serve --configuration development --host 127.0.0.1 --port 4600\" http://127.0.0.1:4600 \"E2E_BASE_URL=http://127.0.0.1:4600 npx playwright test\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@tailwindcss/postcss": "^4.1.13",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.16",
    "@angular/cli": "^16.2.16",
    "@angular/compiler-cli": "^16.2.0",
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.55.0",
    "@storybook/addon-a11y": "^8.2.9",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/angular": "^8.2.9",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/jasmine": "~4.3.0",
    "angular-eslint": "20.3.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.35.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "msw": "^2.11.2",
    "pa11y": "^9.0.0",
    "pa11y-ci": "^4.0.1",
    "postcss": "^8.5.6",
    "start-server-and-test": "^2.1.2",
    "storybook": "^8.2.9",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.1.3",
    "typescript-eslint": "8.40.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended",
      "plugin:storybook/recommended",
      "plugin:storybook/recommended"
    ]
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
